    "import matplotlib.pylab as plt \n",
    "import numpy as np \n",
    "x = np.arange(-8, 8, 0.1) \n",
    "f = 1 / (1 + np.exp(-x)) \n",
    "plt.plot(x, f) \n",
    "plt.xlabel('x') \n",
    "plt.ylabel('f(x)') \n",
    "plt.show()"
